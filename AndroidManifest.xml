<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="org.ros.android.sensors_driver" android:versionCode="2" android:versionName="0.2">
	<uses-sdk android:minSdkVersion="9" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<application android:icon="@drawable/sensor_icon" android:label="@string/app_name"
		android:debuggable="false">
		<activity android:name="MainActivity" android:label="@string/app_name"
			android:screenOrientation="landscape" android:configChanges="orientation|keyboardHidden">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="org.ros.android.MasterChooser"></activity>
		<service android:enabled="true" android:name=".SensorService" android:icon="@drawable/sensor_icon" android:label="@string/service_name" />
	</application>
</manifest>